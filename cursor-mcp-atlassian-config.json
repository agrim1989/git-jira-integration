{
  "mcpServers": {
    "mcp-atlassian": {
      "command": "npx",
      "args": [
        "mcp-atlassian"
      ],
      "env": {
        "JIRA_URL": "https://your-domain.atlassian.net",
        "JIRA_USERNAME": "your-email@example.com",
        "JIRA_API_TOKEN": "your-api-token"
      }
    }
  }
}